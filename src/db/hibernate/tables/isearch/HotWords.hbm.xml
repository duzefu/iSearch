<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.hibernate.tables.isearch.HotWords" table="hot_words" catalog="isearch">
        <id name="hid" type="java.lang.Integer">
            <column name="hid" />
            <generator class="identity" />
        </id>
        <property name="hotwords" type="string">
            <column name="hotwords" not-null="true" />
        </property>
        <property name="date" type="date">
            <column name="date" length="0" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
